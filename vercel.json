{
    "version": 2,
    "builds": [
      {
        "src": "web/package.json",
        "use": "@vercel/static-build",
        "config": {
          "distDir": "web/dist"
        }
      },
      {
        "src": "studeio-sarah-lopes/package.json",
        "use": "@vercel/node"
      }
    ],
    "env": {
      "NODE_ENV": "production"
    },
    {
        "rewrites": [
          { "source": "/(.*)", "destination": "/" }
        ]
    }
  }
  